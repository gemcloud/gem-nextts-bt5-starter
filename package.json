{
  "name": "gem-next-ts-base-starter",
  "private": true,
  "scripts": {
    "dev_base": "next dev",
    "dev": "next dev -p 3003",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "cd .. && husky install client/.husky",
    "__No_window_lang:extract": "NODE_ENV=development lingui extract --clean",
    "lang:extract": "cross-env NODE_ENV=development lingui extract --clean",
    "Duplicate_declaration_Trans_lang:extract": "lingui extract --clean",
    "lang:compile": "lingui compile",
    "lang": "yarn lang:extract && yarn lang:compile",
    "post-update": "yarn upgrade --latest"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@lingui/react": "^3.13.0",
    "@types/aos": "^3.0.4",
    "animate.css": "^4.1.1",
    "aos": "^2.3.4",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "cross-env": "^7.0.3",
    "firebase": "^9.6.1",
    "make-plural": "^7.0.0",
    "next": "12.0.7",
    "next-seo": "^4.28.1",
    "next-sitemap": "^1.6.203",
    "npm": "^8.3.0",
    "react": "17.0.2",
    "react-animate-on-scroll": "^2.1.5",
    "react-dom": "17.0.2",
    "react-reveal": "^1.2.2",
    "react-spinners": "^0.11.0",
    "sass": "^1.45.1",
    "styled-components": "^5.3.3",
    "swr": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@lingui/cli": "^3.13.0",
    "@lingui/loader": "^3.13.0",
    "@lingui/macro": "^3.13.0",
    "@types/bootstrap": "^5.1.6",
    "@types/node": "16.11.12",
    "@types/react": "17.0.37",
    "@types/react-animate-on-scroll": "^2.1.5",
    "@types/styled-components": "^5.1.19",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "typescript": "4.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx pretty-quick --staged ng lint ng test",
      "pre-push": "ng build --aot true"
    }
  }
}
